alias cgrep='grep -rin --include=*.{h,c,cpp,cxx}'
alias evince='LC_ALL=C evince'
alias free='LC_ALL=C free'
alias fs='sfind'
alias hgrep='grep -rin --include=*.{h,hc,hpp,hxx}'
alias imgconv='ffmpeg -hide_banner -loglevel error'
alias jc='journalctl --no-pager'
alias ls='ls -la --color=auto'
alias pkg-find='pkg-config --list-all | grep'
alias psmem='sudo psmem.py'
alias sc='sudo systemctl --no-pager'
alias syslog='journalctl -b -f --lines=500'
alias sysprc='sudo sysquery -prc'
alias usc='systemctl --no-pager'
